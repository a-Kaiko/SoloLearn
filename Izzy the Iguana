import java.util.*;
public class Main {

    public static void main(String[] args) {
        System.out.println(Snacks.bringSnacks());
    }

    public static class Snacks {

        public static String bringSnacks() {
            String iguanaAction = "";
            if (getSnacksPoints() > 10)
                iguanaAction = "Come on Down!";
            else
                iguanaAction = "Time to wait";
            return iguanaAction;
        }
    }

    private static int getSnacksPoints() {
        int points = 0;
        for (String s : getList()) {
            switch (s) {
                case "Lettuce" :
                    points += 5;
                    break;
                case "Carrot" :
                    points += 4;
                    break;
                case "Mango" :
                    points += 9;
                    break;
                default :
                    points += 0;
                    break;
            }
        }
        return points;
    }
    private static List<String> getList() {
        String[] snacksBrought = input().split(" ");
        List<String> snacksList;
        snacksList = Arrays.asList(snacksBrought);
        return snacksList;
    }
}
